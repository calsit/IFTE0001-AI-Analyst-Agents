# API Keys Configuration Example
# Copy this file to .env and fill in the actual API keys

# DashScope (Qwen) API Key
DASHSCOPE_API_KEY=your_dashscope_api_key_here
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here

# Optional: DashScope Compatible Mode Configuration
DASHSCOPE_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1  # Use https://dashscope.aliyuncs.com/compatible-mode/v1 for Beijing region
DASHSCOPE_MODEL=qwen3-max

# LLM Sampling Configuration
TEMPERATURE=0.3
MAX_TOKENS=2000

# Data Source Configuration
YAHOO_FINANCE_ENABLED=True
ALPHA_VANTAGE_ENABLED=True
# API Request Configuration
API_TIMEOUT=60
MAX_RETRIES=3

# Other Environment Variables
LOG_LEVEL=INFO
DEBUG=False
